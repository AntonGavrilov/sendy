<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <RootNamespace>Sendy</RootNamespace>
        <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
        <Description>Sendy is small and lightweight solution for CQRS</Description>
        <PackageProjectUrl>https://github.com/AntonGavrilov/sendy</PackageProjectUrl>
        <PackageIcon>48ab3ab4b6eb4a39a1e6a4b84bbbd381.png</PackageIcon>
        <PackageReadmeFile></PackageReadmeFile>
        <RepositoryUrl>https://github.com/AntonGavrilov/sendy</RepositoryUrl>
        <PackageTags>CQRS; Commands and Queries</PackageTags>
        <FileVersion>1.0.0.2</FileVersion>
        <AssemblyVersion>1.0.0.2</AssemblyVersion>
        <Version>1.0.0.2</Version>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <Company>Anton Gavrilov</Company>
        <Authors>Anton Gavrilov</Authors>

    </PropertyGroup>

    <ItemGroup>
      <None Include="..\..\..\Downloads\48ab3ab4b6eb4a39a1e6a4b84bbbd381.png">
        <Pack>True</Pack>
        <PackagePath>\</PackagePath>
      </None>
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="7.0.0-rc.1.22426.10" />
    </ItemGroup>

</Project>
